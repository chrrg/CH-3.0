cls;
$a='123456';
gc($);//清空所有变量
line("a的值为：".$a);
line("a字符串内存变量:".$_000000);
//字符串在运行时会储存至内存变量，内存变量不会被gc($)清空
//$_开头的变量不会被清空

gc(all);//清空所有（$_开头的变量不会被清空）
line(gc($_000000));//回收指定变量

$a="1234";
line("gc($a)前a的值：".$a);
gc($a);
line("gc($a)后a的值：".$a);

def(abc,456);
line("gc(abc)前abc的返回值：".abc);
gc(abc);
line("gc(abc)前abc的返回值：".abc);
//无此函数会报错！